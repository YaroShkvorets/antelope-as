{
  "name": "chaintester",
  "version": "0.1.5",
  "description": "chain tester for AssemblyScript Smart Contract",
  "main": "dist/index.js",
  "author": "learnforpractice <learnforpractice@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/uuosio/ascdk.git"
  },
  "bugs": {
    "url": "https://github.com/uuosio/ascdk/issues"
  },
  "scripts": {
    "test": "jest --no-cache",
    "checks": "tsc -p tsconfig.json --noEmit",
    "build": "rm -rf dist/* && tsc -p tsconfig.json"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  },
  "dependencies": {
    "cross-fetch": "^3.1.5"
  },
  "devDependencies": {
    "@types/jest": "^28.1.6",
    "@types/node": "^18.7.1",
    "jest": "^28.1.3",
    "ts-jest": "^28.0.7",
    "typescript": "^4.7.4"
  }
}
