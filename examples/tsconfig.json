{
  "compilerOptions": {
      "skipLibCheck": true,
      "target": "es6",
      "module": "commonjs",
      "moduleResolution": "node",
      "allowJs": true,
      "resolveJsonModule": true,
      "esModuleInterop": true,
      "types": ["jest", "node", "assembly"],
      "typeRoots": ["./types", "./node_modules/@types", "node_modules/assemblyscript/std", "node_modules/as-chain/types.d.ts"],
  },
  "exclude": ["node_modules", "**/*.spec.ts", "**/*.test.ts"],
  "include": ["./src/**/*.tsx", "./src/**/*.ts"]
}
